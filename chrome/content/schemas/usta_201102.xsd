			<xs:element name="Kz96" type="elster:usta_DefaultKZSTypewithCent" minOccurs="0"/>
			<xs:element name="Kz98" type="elster:usta_DefaultKZSTypewithCent" minOccurs="0"/>
		</xs:all>
	</xs:complexType>
</xs:schema>
